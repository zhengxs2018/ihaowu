    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
